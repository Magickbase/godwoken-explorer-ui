    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.2",
    "@types/node": "14.14.35",
    "@types/phoenix": "1.5.1",
    "@types/react": "17.0.3",
    "autoprefixer": "10.2.5",
    "babel-plugin-istanbul": "6.0.0",
    "cypress": "6.7.1",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "postcss": "8.2.8",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "14.0.0",
    "postcss-nesting": "7.0.1",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "sass": "1.32.8",
    "tailwindcss": "2.0.4",
    "typescript": "4.2.3"
  },
  "nyc": {
    "exclude": [
      "pages/api/__coverage__.js"
